package com.revature;

public class Q02 {

	public static int fibonacci(int n) {
		// fib(n) = fib(n-1) + fib(n-2)
		
		// Handle our base cases
		if (n == 0)
			return 0;
		else if (n == 1)
			return 1;
		
		// Since there are no base cases, perform the recursion
		return fibonacci(n-1) + fibonacci(n-2);
	}
}
